{
  "name": "website-react-examples",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:GetStream/website-react-examples.git",
  "author": "Stream",
  "license": "MIT",
  "scripts": {
    "build:cs": "cd customer-support && yarn && yarn build && mkdir -p ../docs/customer-support && mv build/* ../docs/customer-support",
    "build:t": "cd team-ts && yarn && yarn build && mkdir -p ../docs/team && mv build/* ../docs/team",
    "build:js": "rm -rf ./docs && yarn build:cs && yarn build:t",
    "build:js:public": "rm -rf ./docs && yarn build:cs && yarn build:t",
    "lint": "eslint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start:customer-support": "cd customer-support && yarn start",
    "start:team-ts": "cd team-ts && yarn start",
    "upgrade:customer-support": "cd customer-support && yarn add stream-chat-react stream-chat",
    "upgrade:team-ts": "cd team-ts && yarn add stream-chat-react stream-chat"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jest-dom": "^3.2.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.8",
    "prettier": "^3.1.0"
  },
  "dependencies": {}
}
